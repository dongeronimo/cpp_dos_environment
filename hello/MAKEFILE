DEPS = vector.h
obj = logger.obj main.obj video.obj input.obj utils.obj sprite.obj
bin = hello.exe

CC = wpp386
CFLAGS = -d0 -s -zq -4s -er -oa -ob -oc -e=10 -oh -ol+ -om -or -ot
LD = wlink

$(bin): $(obj)
	$(LD) name $@ file *.obj

.c.obj:
	$(CC) -fo=$@ $(CFLAGS) $<

clean: .symbolic
	del *.obj
	del $(bin) 
